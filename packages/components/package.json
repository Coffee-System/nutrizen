{
  "name": "@nutrizen/components",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --clean",
    "dev": "tsup src/index.ts --dts --format cjs,esm --watch"
  },
  "dependencies": {
    "@nutrizen/constants": "*",
    "@nutrizen/types": "*",
    "@nutrizen/ui": "*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@nutrizen/tsconfig": "*",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "typescript": "^5.6.3"
  }
}
