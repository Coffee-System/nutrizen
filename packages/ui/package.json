{
  "name": "@nutrizen/ui",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format cjs,esm --clean",
    "dev": "tsup src/index.ts --dts --format cjs,esm --watch"
  },
  "dependencies": {
    "react": "19.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Coffee-System/nutrizen.git"
  },
  "bugs": {
    "url": "https://github.com/Coffee-System/nutrizen/issues"
  },
  "homepage": "https://nutrizen.dev",
  "devDependencies": {
    "@types/react": "^19",
    "typescript": "^5"
  }
}
